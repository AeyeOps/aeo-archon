# Images (publish or pull your own)
ARCHON_SERVER_IMAGE=ghcr.io/your-org/archon-server:latest
ARCHON_MCP_IMAGE=ghcr.io/your-org/archon-mcp:latest
ARCHON_AGENTS_IMAGE=ghcr.io/your-org/archon-agents:latest
ARCHON_FRONTEND_IMAGE=ghcr.io/your-org/archon-frontend:latest

# Supabase (Cloud or local CLI)
# Cloud example
# SUPABASE_URL=https://<project>.supabase.co
# SUPABASE_URL_CONTAINER=https://<project>.supabase.co
# Local CLI defaults
SUPABASE_URL=http://127.0.0.1:54321
SUPABASE_URL_CONTAINER=http://host.docker.internal:54321
SUPABASE_SERVICE_KEY=

# Host and ports
HOST=localhost
ARCHON_SERVER_PORT=8181
ARCHON_MCP_PORT=8051
ARCHON_AGENTS_PORT=8052
ARCHON_UI_PORT=3737

# Frontend
VITE_ALLOWED_HOSTS=
VITE_SHOW_DEVTOOLS=false
PROD=true

# Observability
OTEL_EXPORTER_OTLP_ENDPOINT=http://localhost:4318
OTEL_EXPORTER_OTLP_PROTOCOL=http/protobuf
OTEL_EXPORTER_OTLP_ENDPOINT_CONTAINER=http://openobserve:4318
OTEL_TRACES_EXPORTER=otlp
OTEL_LOGS_EXPORTER=otlp
OTEL_METRICS_EXPORTER=otlp
OTEL_TRACES_SAMPLER=always_on
OTEL_SERVICE_NAME_ARCHON_SERVER=archon-server
OTEL_SERVICE_NAME_ARCHON_MCP=archon-mcp
OTEL_SERVICE_NAME_ARCHON_AGENTS=archon-agents
